{
    "al.codeAnalyzers": [
        "${PerTenantExtensionCop}",
        "${CodeCop}",
        "${UICop}"
    ],
    "al.enableCodeAnalysis": true,
    "CRS.ObjectNamePrefix": "",
    "CRS.RemovePrefixFromFilename": true,
    "CRS.RenameWithGit": true,
    "CRS.FileNamePattern": "<ObjectNameShort>.<ObjectTypeShort>.al",
    "CRS.OnSaveAlFileAction": "DoNothing",
    "CRS.DisableDefaultAlSnippets": true,
    "CRS.AlSubFolderName": "src"
}